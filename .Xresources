! after changing this file, run: xrdb .Xresources

! Ubuntu fonts rock
*faceName:     Ubuntu Mono
*faceSize:     16
! Bold does not
*colorBDMode:  true

URxvt*scrollBar: false
URxvt*font: xft:Ubuntu Mono:size=14:antialias=true:hinting=true
URxvt*boldFont: xft:Ubuntu Mono:antialias=true:hinting=true
URxvt*letterSpace: -1

! UTF-8 anyhow anywhere anytime
*utf8: 1

! make alt+f/b work
xterm*eightBitInput: false

! include style
#include ".xcolors/solarized"
