[user]
	name = Vit Brunner
	email = vit.brunner@gmail.com

[core]
	excludesfile = ~/.gitignore

[color]
	ui = auto

[alias]
	br = branch
	ba = branch -a
	bd = branch -D
	st = status
	ci = commit
	cia = commit -a
	co = checkout
	lg = log --summary
	lf = log --full-diff -p
	d = diff
	dc = diff --cached
	pom = push origin master
